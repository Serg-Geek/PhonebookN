handlers = java.util.logging.FileHendler, java.util.logging.ConsoleHandler

java.util.logging.FileHendler.level = INFO
java.util.logging.FileHendler.formatter = java.util.logging.SimpleFormatter
java.util.logging.FileHendler.append = true
java.util.logging.FileHendler.pattern= log.%u.%g.txt

java.util.logging.ConsoleHendler.level = INFO
java.util.logging.ConsoleHendler.formatter = java.util.logging.SimpleFormatter